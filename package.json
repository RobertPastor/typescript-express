{
  "name": "typescript-express",
  "version": "1.0.0",
  "description": "express server written in typescript - launch with npm run dev",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon dist/app.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "test": "npm jest"
  },
  "keywords": [
    "typescript",
    "express"
  ],
  "author": "Robert PASTOR",
  "license": "ISC",
  "dependencies": {
    "@types/express-fileupload": "^1.1.3",
    "@types/pg": "^7.14.3",
    "ejs": "^3.1.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "pg": "^8.0.3",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.3"
  }
}
